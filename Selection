def selection(salary):
    n = len(salary)
    for i in range(n - 1):
        min_index = i
        for j in range(i + 1, n):
            if salary[j] < salary[min_index]:
                min_index = j
        salary[i], salary[min_index] = salary[min_index], salary[i]
    print("\nSelection Sort:", salary)
    print("Top 5 Salaries:", salary[-5:])


salary = []

n = int(input("How many employees? "))
for i in range(n):
    sal = float(input("Enter salary of employee {}: ".format(i + 1)))
    salary.append(sal)

selection(salary)
